/// sPlus.js
(function () {
    window.sPlusBookmarkletSourcePath = "https://splus.r58playz.dev/";
    fetch(window.sPlusBookmarkletSourcePath+"loader.js", {cache: "no-store"}).then(r=>r.text()).then(r=>eval(r));
})();

/// sPlusLocal.js
(function () {
    window.sPlusBookmarkletSourcePath = "http://localhost:8000/";
    fetch(window.sPlusBookmarkletSourcePath+"loader.js", {cache: "no-store"}).then(r=>r.text()).then(r=>eval(r));
})();
